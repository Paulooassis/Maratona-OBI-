#include <stdio.h>
#include <string.h>

int main(){
    int num;

    do{
       scanf("%d", &num);
    }while(num < 1 || num > 10);


    if(num <= 5){
        if(num == 0){
            printf("*");
        }else{
            for(int i=0; i<num; i++){
                printf("I");
            }
        }
        printf("\n");
        printf("*");
    }else{
        printf("IIIII\n");
        for(int i=0; i<num-5; i++){
            printf("I");
        }
    }
}
