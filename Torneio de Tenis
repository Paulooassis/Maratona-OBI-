#include <stdio.h>
#include <stdlib.h>

int main(){
    char letra[6];
    int cont = 0;

    for(int i=0; i<6; i++){
        scanf(" %c", &letra[i]);
    }

    for(int i=0; i<6; i++){
        if(letra[i] == 'V'){
            cont++;
        }
    }

    if(cont == 6 || cont == 5){
        printf("1");
    }else if(cont == 4 || cont == 3){
        printf("2");
    }else if(cont == 2 || cont == 1){
        printf("3");
    }else{
        printf("-1");
    }

}
